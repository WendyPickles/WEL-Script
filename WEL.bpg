MAKE = $(ROOT)\bin\make.exe -$(MAKEFLAGS) -f$**
DCC = $(ROOT)\bin\dcc32.exe $**
BRCC = $(ROOT)\bin\brcc32.exe $**
#------------------------------------------------------------------------------
PROJECTS = WelTestsProject.exe Ide.exe
#------------------------------------------------------------------------------
default: $(PROJECTS)
#------------------------------------------------------------------------------

WelTestsProject.exe: WelTestsProject.dpr
  $(DCC)

Ide.exe: Ide.dpr
  $(DCC)


